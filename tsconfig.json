{
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/scss.d.ts",
    "src/cssvars.d.ts"
  ],
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "ESNext",
    "esModuleInterop": true,
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "temp-ts-out",
    "declarationMap": true,
    "jsx": "react-jsx"
  }
}
